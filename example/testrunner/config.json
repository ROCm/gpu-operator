{
  "TestConfig": {
    "GPU_HEALTH_CHECK": {
      "TestLocationTrigger": {
        "global": {
          "TestParameters": {
            "AUTO_UNHEALTHY_GPU_WATCH": {
              "TestCases": [
                {
                  "Recipe": "gst_single",
                  "Iterations": 1,
                  "StopOnFailure": true,
                  "TimeoutSeconds": 1200,
                  "Arguments": "--parallel"
                }
              ],
	      "LogsExportConfig": [
	        {
		  "Provider": "Aws",
		  "BucketName": "aws-bucket-name",
		  "SecretName": "aws-secret"
		},
		{
		  "Provider": "Azure",
		  "BucketName": "azure-bucket-name",
		  "SecretName": "azure-secret"
		}
	      ]
            }
          }
        }
      }
    }
  }
}
